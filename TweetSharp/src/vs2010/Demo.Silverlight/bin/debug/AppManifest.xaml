<Deployment xmlns="http://schemas.microsoft.com/client/2007/deployment" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" EntryPointAssembly="Demo.Silverlight" EntryPointType="Demo.Silverlight.App" RuntimeVersion="4.0.50826.0">
